                    <div id="map" style="width:1100px;height:400px;">
                    </div>
                    <script>

                        function initMap() {
                            var myLatLng = { lat: @Model.mekan1.LAT, lng: @Model.mekan1.LNG };
                            var myLatLng2 = { lat: @Model.mekan2.LAT, lng: @Model.mekan2.LNG };
                            var myLatLng3 = { lat: @Model.mekan3.LAT, lng: @Model.mekan3.LNG };
                            var myLatLng4 = { lat: @Model.mekan4.LAT, lng: @Model.mekan4.LNG };
                            var myLatLng5 = { lat: @Model.mekan5.LAT, lng: @Model.mekan5.LNG };
                            var map = new google.maps.Map(document.getElementById('map'), {
                                zoom: 13,
                                center: myLatLng
                            });
                            var infowindow = new google.maps.InfoWindow({
                                content: "@Model.mekan1.mekanAd"
                            });
                            var infowindow2 = new google.maps.InfoWindow({
                                content: "@Model.mekan2.mekanAd"
                            });
                            var infowindow3 = new google.maps.InfoWindow({
                                content: "@Model.mekan3.mekanAd"
                            });
                            var infowindow4 = new google.maps.InfoWindow({
                                content: "@Model.mekan4.mekanAd"
                            });
                            var infowindow5 = new google.maps.InfoWindow({
                                content: "@Model.mekan5.mekanAd"
                            });

                            var marker = new google.maps.Marker({
                                position: myLatLng,
                                map: map,
                                title: '@Model.mekan1.mekanAd'
                            });
                            var marker2 = new google.maps.Marker({
                                position: myLatLng2,
                                map: map,
                                title: '@Model.mekan2.mekanAd'
                            });
                            var marker3 = new google.maps.Marker({
                                position: myLatLng3,
                                map: map,
                                title: '@Model.mekan3.mekanAd'
                            });
                            var marker4 = new google.maps.Marker({
                                position: myLatLng4,
                                map: map,
                                title: '@Model.mekan4.mekanAd'
                            });
                            var marker5 = new google.maps.Marker({
                                position: myLatLng5,
                                map: map,
                                title: '@Model.mekan5.mekanAd'
                            });

                            marker.addListener('click', function () {
                                infowindow.open(map, marker);
                            });

                            marker2.addListener('click', function () {
                                infowindow2.open(map, marker2);
                            });

                            marker3.addListener('click', function () {
                                infowindow3.open(map, marker3);
                            });

                            marker4.addListener('click', function () {
                                infowindow4.open(map, marker4);
                            });

                            marker5.addListener('click', function () {
                                infowindow5.open(map, marker5);
                            });
                        }
                    </script>
                    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBY04YUOytfoNsdAWu8jAiTyvcJwE586Wo&amp;callback=initMap" async="" defer=""></script>
